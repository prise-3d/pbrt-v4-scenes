AttributeBegin
    AreaLightSource "diffuse"
        "rgb L" [ 7000 5450 3630 ]
    NamedMaterial "Luminaire2Material"
    Shape "plymesh"
        "string filename" [ "geometry/bidir_6.ply" ]
AttributeEnd

AttributeBegin
    AreaLightSource "diffuse"
        "rgb L" [ 500 500 500 ]
    NamedMaterial "Luminaire1Material"
    Shape "plymesh"
        "string filename" [ "geometry/bidir_7.ply" ]
